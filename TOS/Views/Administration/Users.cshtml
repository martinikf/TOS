@model IEnumerable<TOS.Models.ApplicationUser>

@{
    ViewData["Title"] = "Administration - Users";
}

<h1>Administration</h1>

<form asp-action="Users" method="get">
    <input type="text" name="searchString" value="@ViewData["searchString"]"/>
    <input type="submit" value="Search"/>
</form>


@foreach (var user in Model)
{
    <p>
        @user.FirstName - @user.LastName - @user.Email - <a asp-controller="Administration" asp-action="EditRoles" asp-route-id="@user.Id">Edit Roles</a>
    </p>
}