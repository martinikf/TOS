@model IEnumerable<TOS.Models.Programme>

@{
    ViewData["Title"] = "Administration - Programmes";
}

<h1>Administration</h1>

<ul>
    <li><a asp-controller="Administration" asp-action="CreateProgramme">Create</a></li>
</ul>


@foreach (var programme in Model)
{
    <span>
        @programme.Name - @programme.Active - @programme.NameEng - @programme.Type.ToString() - <a asp-controller="Administration" asp-action="EditProgramme" asp-route-id="@programme.ProgrammeId">Edit</a>
    </span>
    <br/>
  
}